{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview --host",
    "test:e2e": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed'",
    "test:e2e:electron": "npm run test:e2e",
    "test:e2e:chrome": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed --browser chrome'",
    "test:e2e:firefox": "start-server-and-test preview localhost:4173 'cypress run --e2e --headed --browser firefox'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' localhost:4173 'cypress open --e2e'",
    "test:unit": "vitest run",
    "test:unit:dev": "vitest",
    "lint": "eslint src/ tests/",
    "lint:fix": "npm run lint -- --fix",
    "lint:report": "npm run lint -- --output-file eslint_report.json --format json"
  },
  "dependencies": {
    "@tailwindcss/vite": "4.1.10",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "13.3.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "lucide-vue-next": "0.513.0",
    "mitt": "3.0.1",
    "moment": "2.30.1",
    "pinia": "3.0.3",
    "pretty-bytes": "7.0.0",
    "primeflex": "4.0.0",
    "primeicons": "7.0.0",
    "primevue": "3.53.1",
    "reka-ui": "2.3.1",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.10",
    "tw-animate-css": "1.3.4",
    "vue": "3.5.16",
    "vue-router": "4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@iconify-json/radix-icons": "1.2.2",
    "@iconify/vue": "5.0.0",
    "@rushstack/eslint-patch": "1.11.0",
    "@tanstack/vue-table": "8.21.3",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "5.2.4",
    "@vitest/eslint-plugin": "1.2.7",
    "@vitest/ui": "3.2.4",
    "@vue/eslint-config-prettier": "10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "axios": "1.9.0",
    "cypress": "14.4.1",
    "cypress-mochawesome-reporter": "3.8.2",
    "cypress-multi-reporters": "2.0.5",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-cypress": "^4.3.0",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^26.1.0",
    "mocha-junit-reporter": "2.2.1",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "5.0.0",
    "mochawesome-report-generator": "6.2.0",
    "prettier": "3.5.3",
    "sass": "1.89.2",
    "start-server-and-test": "2.0.12",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "6.3.5",
    "vitest": "3.2.4",
    "vue-tsc": "2.2.10"
  }
